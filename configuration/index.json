{
  "job": {
    "from": "Dynamodb",
    "to": "Hdfs",
    "resultSource": {
      "type": "FileResultsSource",
      "path": "logs/results"
    },
    "buckets": [{
      "name": "PaxLabDynamoHive",
      "itemsPerJson": 10
    }]
  },
  "aws": {
    "region": "us-east-1"
  },
  "hdfs": {
    "user": "webuser",
    "host": "localhost",
    "port": 50070,
    "path": "/webhdfs/v1",
    "writePath": "/datalake/warehouse/tablespace/managed/hive/external_json"
  }
}